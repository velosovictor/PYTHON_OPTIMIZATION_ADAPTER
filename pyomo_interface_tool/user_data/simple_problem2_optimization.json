{
  "time_discretization": {
    "dt": 0.5,
    "final_time": 1.0,
    "method": "backward_euler"
  },
  "state_variables": ["x", "v"],
  "initial_conditions": {
    "x": 1.0,
    "v": 0.0
  },
  "equations": [
    "x(t+1) = x(t) + dt * v(t+1)",
    "v(t+1) = v(t) + dt * (-k_eff(t) * x(t+1) - c * v(t+1)) / m"
  ],
  "parameters": {
    "m": 1.0,
    "c": 0.1
  },
  "optimization_variables": ["k_eff"],
  "bounds": {
    "k_eff": [1.0, 10.0],
    "x": [-5.0, 5.0],
    "v": [-10.0, 10.0]
  },
  "optimization_objectives": {
    "minimize_energy": {
      "type": "minimize", 
      "expression": "sum(k_eff(t) * x(t)**2 for t in time_steps)",
      "weight": 1.0
    }
  },
  "endpoint_constraints": {
    "x_final": 0.0
  },
  "discrete_parameters": {},
  "discrete_logic": {},
  "problem_info": {
    "name": "Simple Optimization - Spring System",
    "description": "Find optimal spring stiffness k_eff to reach target position while minimizing energy"
  }
}
